{
    "python.defaultInterpreterPath": "/usr/bin/python3.10",
    "python.formatting.provider": "autopep8",
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnType": true
    },
    "ruff.interpreter": [
        "/usr/bin/python3.10"
    ],
    "ruff.args": [
        "--line-length=100"
    ],
    "cmake.configureOnOpen": true,
    "clangd.path": "/home/novakovic/.config/VSCodium/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/16.0.2/clangd_16.0.2/bin/clangd",
    "editor.cursorSmoothCaretAnimation": "explicit",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "FiraCode Nerd Font Mono Retina, FiraCode Nerd Font Propo Retina",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.lineNumbers": "relative",
    "editor.overviewRulerBorder": false,
    "editor.guides.highlightActiveIndentation": true,
    "editor.renderLineHighlight": "gutter",
    "editor.roundedSelection": false,
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "telemetry.telemetryLevel": "off",
    "window.menuBarVisibility": "compact",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Tokyo Night Storm",
    "workbench.editor.tabCloseButton": "off",
    "workbench.iconTheme": "eq-material-theme-icons-darker",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.startupEditor": "none",
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false
    },
    "vim.matchpairs": "(:),{:},[:],<:>",
    "vim.showMarksInGutter": true,
    "vim.autoindent": true,
    "vim.highlightedyank.enable": true,
    "vim.smartRelativeLine": true,
    "vim.textwidth": 110,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.surround": true,
    "vim.scroll": 16,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "s",
                "b"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                "workbench.view.search"
            ]
        },
        {
            "before": [
                "<leader>",
                "g"
            ],
            "commands": [
                "workbench.scm.focus"
            ]
        },
        {
            "before": [
                "e",
                "?"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "x"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "n"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "p"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "h"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "n",
                "z",
                "z",
                "z",
                "v"
            ],
            "after": [
                "n"
            ]
        },
        {
            "before": [
                "N",
                "z",
                "z",
                "z",
                "v"
            ],
            "after": [
                "N"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "p",
                "v"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "p",
                "h"
            ],
            "commands": [
                "workbench.action.splitEditorOrthogonal"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ],
            "silent": true
        },
        {
            "before": [
                "<leader>",
                "g",
                "D"
            ],
            "commands": [
                "editor.action.goToDeclaration"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "I"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "r"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        }
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [],
            "commands": []
        }
    ],
    "terminal.integrated.inheritEnv": false,
}